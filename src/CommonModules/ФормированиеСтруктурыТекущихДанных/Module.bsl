	СтруктураТекДанных.Вставить("ТребуемоеЗначениеЖилаСиняя", ТекущиеДанные.ТребуемоеЗначениеЖилаСиняя);
	СтруктураТекДанных.Вставить("ТребуемоеЗначениеЖилаЗаземления", ТекущиеДанные.ТребуемоеЗначениеЖилаЗаземления);
	СтруктураТекДанных.Вставить("ТребуемоеЗначениеИзоляцииЖилаОсновная", ТекущиеДанные.ТребуемоеЗначениеИзоляцииЖилаОсновная);
	СтруктураТекДанных.Вставить("ТребуемоеЗначениеИзоляцииЖилаКоричневая", ТекущиеДанные.ТребуемоеЗначениеИзоляцииЖилаКоричневая);
	СтруктураТекДанных.Вставить("ТребуемоеЗначениеИзоляцииЖилаЧерная", ТекущиеДанные.ТребуемоеЗначениеИзоляцииЖилаЧерная);
	СтруктураТекДанных.Вставить("ТребуемоеЗначениеИзоляцииЖилаСиняя", ТекущиеДанные.ТребуемоеЗначениеИзоляцииЖилаСиняя);
	СтруктураТекДанных.Вставить("ТребуемоеЗначениеИзоляцииЖилаЗаземления", ТекущиеДанные.ТребуемоеЗначениеИзоляцииЖилаЗаземления);
	СтруктураТекДанных.Вставить("Тара", ТекущиеДанные.Тара); 
	СтруктураТекДанных.Вставить("ШтампОТК", ТекущиеДанные.ШтампОТК); 
	СтруктураТекДанных.Вставить("ЦветКабеля", ТекущиеДанные.ЦветКабеля);
	СтруктураТекДанных.Вставить("МаркаНапряжение", ТекущиеДанные.МаркаНапряжение);
	СтруктураТекДанных.Вставить("Напряжение", ТекущиеДанные.МаркаНапряжение);
	СтруктураТекДанных.Вставить("ПризнакРучныеИзмененияТребуемыхЗначений", ТекущиеДанные.ПризнакРучныеИзмененияТребуемыхЗначений); 
	СтруктураТекДанных.Вставить("ВидСертификата", ТекущиеДанные.ВидСертификата);	
	Возврат СтруктураТекДанных;
	
КонецФункции
